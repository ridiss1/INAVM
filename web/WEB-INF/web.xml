<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>FormCreatContenaire</servlet-name>
        <servlet-class>Controller.FormCreatContenaire</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AuthentificationServlet</servlet-name>
        <servlet-class>Controller.AuthentificationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VmProfUpdateServlet</servlet-name>
        <servlet-class>Controller.VmProfUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListTemplates</servlet-name>
        <servlet-class>Controller.ListTemplates</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateTemplate</servlet-name>
        <servlet-class>Controller.CreateTemplate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveInfoServlet</servlet-name>
        <servlet-class>Controller.SaveInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormCreatContenaire</servlet-name>
        <url-pattern>/FormCreatContenaire</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VmProfUpdateServlet</servlet-name>
        <url-pattern>/vmProfModify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuthentificationServlet</servlet-name>
        <url-pattern>/accueil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuthentificationServlet</servlet-name>
        <url-pattern>/AuthentificationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListTemplates</servlet-name>
        <url-pattern>/ListTemplates</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateTemplate</servlet-name>
        <url-pattern>/CreateTemplate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveInfoServlet</servlet-name>
        <url-pattern>/SaveInfoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
